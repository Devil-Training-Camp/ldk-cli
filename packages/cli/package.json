{
  "name": "@ldk/cli",
  "version": "0.0.0",
  "private": false,
  "description": "a vue cli",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "files": [
    "lib",
    "bin.js"
  ],
  "bin": {
    "ldk-cli": "./bin.js"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development pnpm dev:tsx",
    "c:temp": "pnpm dev temp",
    "c:plugin": "pnpm dev plugin",
    "c:create": "pnpm dev create ../../lib -f",
    "dev:tsnode": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./src/index.ts",
    "dev:node": "pnpm build && node ./bin.js",
    "dev:tsx": "tsx ./src/index.ts",
    "build": "rimraf ./lib && tsc"
  },
  "keywords": [
    "vue3",
    "cli"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devil-Training-Camp/ldk-cli.git",
    "directory": "packages/cli"
  },
  "author": {
    "name": "grey-coat",
    "email": "461252486@qq.com"
  },
  "license": "MIT",
  "dependencies": {
    "@ldk/plugin-manager": "workspace:*",
    "@ldk/shared": "workspace:*",
    "@ldk/template-manager": "workspace:*",
    "@ldk/plugin-core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "inquirer": "^9.2.23"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.12.0"
  }
}
